#main
	script
		window.onload = function() {
			///console.log( "ready!" );
		};

	include userbar
	#display
			div
				table.main-topic(style='width:100%; ')
					-for (var id in data)
						- var item = data[id]
							tr(style='padding:5px; ')
								td 
									div.topic-details
										a(href='/category/#{item.url_slag}')=item.name 
								td #{moment(item.created_at).format("DD-MMM-YY HH:mm")}
								td #{moment(item.updated_at).format("DD-MMM-YY HH:mm")}

					thead
						tr
							th.head-topic  Topic
							th Created_at
							th Updated_at

			include footer